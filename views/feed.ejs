<%- include('partials/header') -%>
  <div class="feedContainer container">
    <% for(let i=0; i<posts.length; i++) {%>
      <div class="feedPost">
        <div class="likesBar">
          <span>&uparrow;</span>
          <span>10,234</span>
          <span>&downarrow;</span>
        </div>
        <div class="feedContentContainer">
          <div>
            <h2 id="feedTitle"><%=posts[i].title %></h2>
          </div>
          <div class="feedImgContainer">
            <a class="" href="/post/<%= posts[i]._id%>">
              <img class="feedImg" src="<%= posts[i].image%>">
            </a>
          </div>
          <div class="commentsAndShare">
            <h4>Comments</h4>
          </div>
        </div>
      </div>
      <% } %>
  </div>
  <%- include('partials/footer') -%>